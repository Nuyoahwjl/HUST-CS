fun createTable (M:int, N:int): int list =
    if M > N then []
    else M :: createTable(M + 1, N);

fun printIntTable ( [] ) = ()
  | printIntTable ( x::xs ) = 
    let
	val tmp = printInt(x)
    in
	printIntTable(xs)
    end;

val M = getInt();  
val N = getInt();  

val result = createTable(M, N);  
printIntTable(result);
