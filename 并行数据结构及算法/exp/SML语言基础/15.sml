fun findName ([], _) = 0
  | findName ((name, id)::xs, targetId) =
    if id = targetId then name
    else findName (xs, targetId);

val N = getInt();
val names = getIntTable(N); 
val ids = getIntTable(N); 
val pairs = ListPair.zip(names, ids);  
val targetId = getInt(); 

val result = findName(pairs, targetId);
printInt(result);
