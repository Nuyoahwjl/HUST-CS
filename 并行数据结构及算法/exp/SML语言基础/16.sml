fun minCoins ([], amount:IntInf.int) = IntInf.fromInt 0
  | minCoins (x::xs, amount:IntInf.int) =
    let
      val numCoins = IntInf.div (amount, x)
      val remaining = IntInf.mod (amount, x)
    in
      IntInf.+ (numCoins, minCoins (xs, remaining))
    end;

val N = getIntInf();
val money = getIntInfTable(IntInf.toInt N);
val C = getIntInf();

val result = minCoins(money,C);
printIntInf(result);
